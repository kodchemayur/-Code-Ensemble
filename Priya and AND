//#include <algorithm>
#include <iostream>
#include <bits/stdc++.h>
using namespace std;
//#include<stdio.h>
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
   int t;
    cin >> t;
    while (t--) {
      
    int n;cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int sum=0;
    if(n==1){
        cout<<"0"<<"\n";
    }else{
   for(int i=0;i<n-1;i++){
    for(int j=i+1;j<n;j++){
        if((arr[i]&arr[j])==arr[i]){
            sum++;
        }
    }
}
        cout<<sum<<"\n";
    }

    }
    
}










